    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@prisma/client": "^4.2.1",
    "apiframework": "github:modscleo4/apiframework",
    "dotenv": "^16.0.1"
  }
