    "@types/chai": "^4.3.3",
    "@types/expect": "^24.3.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "^18.7.11",
    "autocannon": "^7.9.0",
    "chai": "^4.3.6",
    "mocha": "^10.0.0",
    "mocha-suppress-logs": "^0.3.1",
    "nodemon": "^2.0.19",
    "prisma": "^4.6.1",
    "supertest": "^6.2.4",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "ttypescript": "^1.5.15",
    "typescript": "^4.7.4",
    "typescript-transform-paths": "^3.3.1"
  },
